  "version": "1.0.0",
  "description": "Frontend for TADA built using React.js",
  "main": "webpack.config.js",
  "scripts": {
    "dev": "mkdir -p dist && cp index.html dist/index.html && node_modules/.bin/webpack-dev-server --display-error-details --devtool source-map --inline --progress --colors --content-base dist --history-api-fallback",
